<view class="all_order"><view class="list_info" a:for="{{list}}" a:for-item="item" a:for-index="__i0__" a:key="id"><view class="list_header"><view class="order_id">订单编号：<text>{{item.id}}</text></view><view class="order_time">{{item.time}}</view></view><view class="list_container"><view class="_ul"><view class="_li" a:for="{{item.commodity}}" a:for-item="item1" a:for-index="index1" a:key="index1"><text class="left"><text class="name_number">{{item1.name+"*"+item1.numbner}}</text></text><text class="right"><text>￥</text><text>{{item1.price}}</text></text></view></view></view><view class="total"><text>总计：</text><text class="total_price">{{"￥"+item.totalPrice}}</text></view><view class="footer_btn"><button class="sh" type="default" size="mini" data-event-opts="{{[['tap',[['shChange',['$event']]]]]}}" onTap="__e">售后</button></view></view></view>