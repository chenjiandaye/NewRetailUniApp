<view class="all_order"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view class="list_info"><view class="list_header"><view class="order_id">订单编号：<text>{{item.id}}</text></view><view class="order_time">{{item.time}}</view></view><view class="list_container"><view class="_ul"><block wx:for="{{item.commodity}}" wx:for-item="item1" wx:for-index="index1" wx:key="index1"><view class="_li"><text class="left"><text class="name_number">{{item1.name+"*"+item1.numbner}}</text></text><text class="right"><text>￥</text><text>{{item1.price}}</text></text></view></block></view></view><view class="total"><text>总计：</text><text class="total_price">{{"￥"+item.totalPrice}}</text></view><view class="footer_btn"><button class="sh" type="default" size="mini" data-event-opts="{{[['tap',[['shChange',['$event']]]]]}}" bindtap="__e">售后</button></view></view></block></view>